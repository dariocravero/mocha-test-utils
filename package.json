{
  "name": "mocha-test-utils",
  "version": "0.2.1",
  "description": "mocha.js test utils",
  "main": "index.js",
  "scripts": {
    "test": ".bin/test",
    "compile": "babel --only `find . -name '*.es6' -type f -not -path './node_modules/*'` -d . .",
    "prepublish": "npm run compile",
    "postpublish": "find . -name '*.js' -type f -not -path './node_modules/*' -exec rm {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dariocravero/mocha-test-utils"
  },
  "keywords": [
    "mocha",
    "test",
    "utils"
  ],
  "author": "Dar√≠o Javier Cravero <dario@uxtemple.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dariocravero/mocha-test-utils/issues"
  },
  "homepage": "https://github.com/dariocravero/mocha-test-utils",
  "devDependencies": {
    "babel": "^5.1.11",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1"
  }
}
